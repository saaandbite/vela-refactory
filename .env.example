# GitHub Token for integrations
GITHUB_TOKEN=ghp_your_github_token_here
# GitHub OAuth for authentication
AUTH_GITHUB_CLIENT_ID=yo
AUTH_GITHUB_CLIENT_SECRET=your_github_client_secret_here

# CSV Analyzer - AI-Powered Data Analysis
# Required: OpenRouter API key for sentiment analysis and topic clustering
# Get it from: https://openrouter.ai/keys
REACT_APP_OPENROUTER_API_KEY=sk_open_your_openrouter_api_key_here

# Optional: Jina AI for URL content extraction in CSV analysis
# Get it from: https://jina.ai/reader
REACT_APP_JINA_API_KEY=jina_your_jina_api_key_here

# PostgreSQL (using docker-compose defaults)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=vela
POSTGRES_PASSWORD=vela_dev_password
POSTGRES_DB=vela

# Redis (using docker-compose defaults)
REDIS_HOST=localhost
REDIS_PORT=6379
